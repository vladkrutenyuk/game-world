{
  "name": "@vladkrutenyuk/game-world",
  "version": "0.0.1-alpha.10",
  "author": "Vlad Krutenyuk <vladislove2402@gmail.com> (https://kvy.world)",
  "license": "MIT",
  "description": "Game world for `three.js`.",
  "homepage": "https://github.com/vladkrutenyuk/game-world",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladkrutenyuk/game-world"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/build/index.cjs",
  "module": "./dist/build/index.module.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/build/index.module.js",
      "require": "./dist/build/index.cjs",
      "types": "./dist/src/index.d.ts"
    },
    "./features": {
      "import": "./dist/examples/features/index.js",
      "types": "./dist/examples/features/index.d.ts"
    },
    "./modules": {
      "import": "./dist/examples/modules/index.js",
      "types": "./dist/examples/modules/index.d.ts"
    }
  },
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:core && npm run build:examples",
    "build:core": "rimraf .parcel-cache && parcel build --dist-dir dist/build && rimraf dist/src/index.d.ts.map",
    "build:examples": "tsc --p ./examples.tsconfig.jsonc --declaration && tsc-alias && tsc --p ./examples.tsconfig.jsonc",
    "demo": "parcel demo/index.html --dist-dir .demo-cache",
    "alpha-release": "npm run build && npm version prerelease --preid=alpha && npm publish"
  },
  "files": [
    "dist/build",
    "dist/examples",
    "dist/src",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "2.11.0",
    "@parcel/transformer-typescript-types": "2.11.0",
    "@types/three": "^0.160.0",
    "@types/three-nebula": "^10.0.3",
    "parcel": "^2.11.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.23.8",
    "@vladkrutenyuk/cannon-es-debugger-pro": "^0.2.4",
    "cannon-es": "^0.20.0",
    "three": "^0.160.0",
    "three-nebula": "^10.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "cannon-es": {
      "optional": true
    },
    "@vladkrutenyuk/cannon-es-debugger-pro": {
      "optional": true
    },
    "three-nebula": {
      "optional": true
    },
    "@babel/runtime": {
      "optional": true
    }
  }
}
